{
    "display_information": {
        "name": "slack-event-sink"
    },
    "features": {
        "bot_user": {
            "display_name": "logbot2",
            "always_online": true
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "channels:history",
                "channels:join",
                "channels:read",
                "chat:write",
                "pins:read",
                "users:read",
                "reactions:read",
                "groups:read",
                "emoji:read",
                "files:read",
                "team:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://50b98aa6c163.ngrok.io/slack/events",
            "bot_events": [
                "channel_archive",
                "channel_created",
                "channel_deleted",
                "channel_id_changed",
                "channel_left",
                "channel_rename",
                "channel_unarchive",
                "emoji_changed",
                "file_change",
                "file_created",
                "file_deleted",
                "file_shared",
                "file_unshared",
                "member_left_channel",
                "message.channels",
                "pin_added",
                "pin_removed",
                "reaction_added",
                "reaction_removed",
                "team_domain_change",
                "team_join",
                "team_rename",
                "user_change",
                "user_profile_changed"
            ]
        },
        "interactivity": {
            "is_enabled": false
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}
